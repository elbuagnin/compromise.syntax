{
  "Unset #Gerunds": {"batchOrder": "1", "term": "#Gerund", "pattern": "#Gerund", "untag": "#Gerund", "tag": "Verbal"},

  "Nouns that are Verbs 1": {"batchOrder": "1", "term": "#Noun", "pattern": "(#Modal|will|do|does|did) #Negative? #Noun", "tag": ["BaseVerb", "PresentTense"]},

  "Nouns that are Verbs 2": {"batchOrder": "1", "term": "#Noun", "pattern": "(#Pronoun && !#Possessive) #Noun", "tag": ["Verb", "PresentTense"]},

  "Adjectives that are Verbs": {"batchOrder": "1", "term": "#Adjective", "pattern": "#Pronoun #Adjective", "untag": "#Adjective", "tag": ["BaseVerb", "PresentTense"]},

  "Adjectives that are Infinitives": {"batchOrder": "1", "term": "to #Adjective", "pattern": "to #Adjective #Determiner", "untag": "#Adjective", "tag": "Infinitive"},

  "#Cardinal as Determiner": {"batchOrder": "1", "term": "#Cardinal", "pattern": "#Cardinal (#Preposition|#Noun)", "tag": "Determiner"},

  "#Cardinal as Pronoun": {"batchOrder": "1", "term": "#Cardinal", "pattern": "#Cardinal #Verb", "tag": "Pronoun"},

  "#Ordinal #Noun": {"batchOrder": "1", "term": "#Ordinal", "pattern": "#Ordinal #Noun", "tag": "#Adjective"},

  "a #Cardinal": {"batchOrder": "1", "term": "a", "pattern": "a #Cardinal", "tag": "#Determiner"},

  "a #Ordinal #Noun": {"batchOrder": "1", "term": "a", "pattern": "a #Ordinal #Noun", "tag": "#Determiner"},

  "Solitary Present Participle": {"batchOrder": "1", "term": "#Verbal", "pattern": "(^#Verbal && #Comma)", "tag": "PresentParticiple"},

  "#Preposition #Verb": {"batchOrder": "1", "term": "#Verb", "pattern": "#Preposition #Verb", "tag": "#Noun"},

  "#Verb #Noun #Verb 1": {"batchOrder": "1", "term": "#Noun", "pattern": "(is|am|are) (#Noun && !#Pronoun) #Verb", "tag": "#Adverb"},

  "#Verb #Noun #Verb 2": {"batchOrder": "1", "term": "#Plural", "pattern": "has (#Plural && !#Pronoun) #Verb", "tag": "#Adverb"},

  "#Verb #Noun #Verb 3": {"batchOrder": "1", "term": "#Singular", "pattern": "have (#Singular && !#Person && !#Pronoun) #Verb", "tag": "#Adverb"}
}
