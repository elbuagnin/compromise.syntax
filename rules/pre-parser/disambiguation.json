{
    "all As Determiner": {"batchOrder": "3", "term": "all", "pattern": "all #Adjective?+ #Noun", "tag": "Determiner"},

    "as if Is Expression": {"batchOrder": "3", "term": "as if", "pattern": "^as if$", "tag": "Expression"},

    "before Is Preposition": {"batchOrder": "3", "term": "before", "pattern": "before (#NounPhrase?+|#Noun?+ #Noun|#Noun|#Pronoun)", "untag": "Conjunction", "tag": "Preposition"},

    "grace IS Not a Name": {"batchOrder": "3", "term": "grace", "pattern": "#Determiner grace", "untag": "Person", "tag": "Singular"},

    "grace IS Not a Name 2": {"batchOrder": "3", "term": "grace", "pattern": "(grace && !@isUpperCase)", "untag": "Person", "tag": "Singular"},

    "#Cardinal is Determiner": {"batchOrder": "3", "term": "#Cardinal", "pattern": "#Cardinal (#Preposition|#Noun)", "tag": "Determiner"},

    "to Is Preposition": {"batchOrder": "3", "term": "to", "pattern": "to #Determiner? (#NounPhrase?+|#Noun?+ #Noun|#Noun|#Pronoun)", "tag": "Preposition", "untag": "Conjunction"},

    "Solitary Present Participle": {"batchOrder": "3", "term": "#Verbal", "pattern": "^#Verbal$", "tag": "PresentParticiple"},

    "Store as Noun": {"batchOrder": "3", "term": "store", "pattern": "(#Determiner|#Possessive) store", "tag": "#Noun"},

    "one as Pronoun 1": {"batchOrder": "3", "term": "one", "pattern": "(#Pronoun|#Person) #Copula #Article one", "tag": "#Pronoun"},

    "one as Pronoun 2": {"batchOrder": "3", "term": "one", "pattern": "#Article one (who|whom)", "tag": "#Pronoun"},

    "#Ordinal #Noun": {"batchOrder": "3", "term": "#Ordinal", "pattern": "#Ordinal #Noun", "tag": "#Adjective"},

    "Perfect Participle Active": {"batchOrder": "3", "term": "having #PastTense", "pattern": "having #PastTense", "tag": ["PerfectParticiple", "Active"]},

    "Perfect Participle Passive": {"batchOrder": "3", "term": "having been #PastTense", "pattern": "having been #PastTense", "tag": ["PerfectParticiple", "Passive"]},

    "#Verb #Negative #Verb": {"batchOrder": "3", "term": "#Verb #Negative #Verb", "pattern": "#Verb #Negative #Verb", "tag": "#VerbPhrase"}
}
