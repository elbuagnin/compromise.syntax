{
  "Determiners": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "4",
    "order": "1",
    "type": "inter-phrase",
    "pattern": "(#Determiner|#Possessive)",
    "tag": {
      "all": "#Determiner"
    }
  },
  "Adjectives": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "4",
    "order": "2",
    "type": "inter-phrase",
    "pattern": "(#Adjective+ && !#Adjectival)",
    "tag": {
      "all": "#Adjectival"
    }
  },
  "Adjective": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "5",
    "order": "2",
    "type": "inter-phrase",
    "pattern": "#Adjective",
    "tag": {
      "all": "#Adjectival"
    }
  },
  "Nouns": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "4",
    "order": "3",
    "type": "inter-phrase",
    "pattern": "#Noun+",
    "tag": {
      "all": "#Nominal"
    }
  },
  "Pronouns": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "4",
    "order": "4",
    "type": "inter-phrase",
    "pattern": "#Pronoun+",
    "tag": {
      "all": "#Nominal"
    }
  },
  "Adverbs": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "4",
    "order": "7",
    "type": "inter-phrase",
    "pattern": "[#Adverb|#Adverb+]",
    "tag": {
      "all": "Adverbial"
    }
  }
}