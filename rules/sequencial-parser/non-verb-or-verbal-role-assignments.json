{
  "Determiners": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "4",
    "order": "1",
    "type": "inter-phrase",
    "pattern": "(#Determiner|#Possessive)",
    "tag": {
      "all": "#Determiner"
    }
  },
  "Adjectives": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "4",
    "order": "2",
    "type": "inter-phrase",
    "pattern": "(#Adjective+ && !#Adjectival)",
    "tag": {
      "all": "#Adjectival"
    }
  },
  "Nouns": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "4",
    "order": "3",
    "type": "inter-phrase",
    "pattern": "(#Noun && !#Nominal)+",
    "tag": {
      "all": "#Nominal"
    }
  },
  "Pronouns": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "4",
    "order": "4",
    "type": "inter-phrase",
    "pattern": "#Pronoun+",
    "tag": {
      "all": "#Nominal"
    }
  },
  "Prepositions": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "4",
    "order": "5",
    "type": "inter-phrase",
    "pattern": "#Preposition+",
    "tag": {
      "all": "PrepositionalPhrase"
    }
  },
  "Not to Infinitive": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "4",
    "order": "6",
    "type": "inter-phrase",
    "pattern": "#Infinitive+",
    "tag": {
      "all": "InfinitivePhrase"
    }
  },
  "Adverbs": {
    "batch": "non-verb-or-verbal-role-assignments",
    "batchOrder": "4",
    "order": "7",
    "type": "inter-phrase",
    "pattern": "#Adverb+",
    "tag": {
      "all": "Adverbial"
    }
  }
}