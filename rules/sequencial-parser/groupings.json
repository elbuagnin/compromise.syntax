{
  "Not to Infinitive": {"batch":"Groupings", "batchOrder": "2",  "order": "1", "type": "intra-phrase", "pattern": "not to #Infinitive", "tag": {"all": ["Infinitive", "Negative"]}},

  "Infinitives": {"batch":"Groupings", "batchOrder": "2",  "order": "2", "type": "intra-phrase", "pattern": "#Infinitive+", "tag": {"all": "Infinitive"}},

  "Comma Nouns 1": {"batch": "Groupings", "batchOrder": "2", "order": "3", "type": "inter-phrase", "pattern": "(#Noun && #Comma)", "tag": {"all": "CommaNoun"}},

  "Comma Nouns 2": {"batch": "Groupings", "batchOrder": "2", "order": "4", "type": "inter-phrase", "pattern": "(#Hyphenated && #Noun)(#Noun && #Comma)", "tag": {"all": "CommaNoun"}},

  "Comma Adjectives": {"batch": "Groupings", "batchOrder": "2", "order": "5", "type": "inter-phrase", "pattern": "(#Adjective && #Comma)", "tag": {"all": "CommaAdjective"}},

  "Comma Adverbs": {"batch": "Groupings", "batchOrder": "2", "order": "6", "type": "inter-phrase", "pattern": "(#Adverb && #Comma)", "tag": {"all": "CommaAdverb"}},

  "Agents 1": {"batch": "Groupings", "batchOrder": "2", "order": "7", "type": "inter-phrase", "pattern": "(#Person|#Actor)", "tag": {"all": "Agent"}},

  "Agents 2": {"batch": "Groupings", "batchOrder": "2", "order": "8", "type": "intra-phrase", "pattern": "(#Pronoun && !#RelativePronoun)", "tag": {"all": "Agent"}},

  "Agents 3": {"batch": "Groupings", "batchOrder": "2", "order": "9", "type": "inter-phrase", "pattern": "(person|people)", "tag": {"all": "Agent"}},

  "Proper Noun String": {"batch": "Groupings", "batchOrder": "2", "order": "10", "type": "intra-phrase", "pattern": "#ProperNoun{2,}", "tag": {"all": ["Agent", "Nouns"]}},

  "Noun String": {"batch": "Groupings", "batchOrder": "2", "order": "11", "type": "intra-phrase", "pattern": "#Noun{2,}", "tag": {"all": "Nouns"}},

  "Adjective String": {"batch": "Groupings", "batchOrder": "2", "order": "12", "type": "intra-phrase", "pattern": "#Adjective{2,}", "tag": {"all": "Adjectives"}},

  "Adverb String": {"batch": "Groupings", "batchOrder": "2", "order": "13", "type": "intra-phrase", "pattern": "#Adverb{2,}", "tag": {"all": "Adverbs"}},

  "Verb String": {"batch": "Groupings", "batchOrder": "2", "order": "14", "type": "intra-phrase", "pattern": "#Verb{2,}", "tag": {"all": "Verbs"}}
}
