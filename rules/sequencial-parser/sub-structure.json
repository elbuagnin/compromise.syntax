{
  "Clauses": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "1",
    "type": "intra-phrase",
    "pattern": "* #Noun+ * #Verbs+ *",
    "tag": {
      "all": "Clause"
    }
  },
  "Participle Clause": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "1",
    "type": "intra-phrase",
    "pattern": "#Participle+ #Clause+",
    "tag": {
      "all": ["ParticipleClause", "SubordinateClause","Adjectival"]
    },
    "modifier": {
      "on": {
        "term": "*",
        "modifies": "#Participle+",
        "termTag": "ObjectOfParticiple"
      }
    }
  },
  "Subject/Verb 2": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "2",
    "type": "intra-phrase",
    "pattern": "#Subject+ #Verbs+ *",
    "tag": {
      "after": {
        "term": "#Subject+",
        "termTag": [
          "Predicate"
        ]
      }
    }
  },
  "Subject/Verb 3": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "3",
    "type": "intra-phrase",
    "pattern": "#Subject+ #Verbs+ *",
    "tag": {
      "after": {
        "term": "#Subject+",
        "termTag": [
          "Predicate"
        ]
      }
    }
  },
  "Noun Subject Compliment": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "4",
    "type": "intra-phrase",
    "pattern": "#Subject+ #Linking+ #Nominal+",
    "tag": {
      "on": {
        "term": "#Nominal+",
        "termTag": "SubjectComplement"
      }
    },
    "modifier": {
      "on": {
        "term": "#Nominal+",
        "modifies": "#Subject+",
        "termTag": "SubjectComplement"
      }
    }
  },
  "Adjective Subject Compliment": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "5",
    "type": "intra-phrase",
    "pattern": "#Subject+ #Linking+ #Adjectival+",
    "tag": {
      "on": {
        "term": "#Adjectival+",
        "termTag": "SubjectComplement"
      }
    },
    "modifier": {
      "on": {
        "term": "#Adjectival+",
        "modifies": "#Subject+",
        "termTag": "SubjectComplement"
      }
    }
  },
  "Subject": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "6",
    "type": "inter-phrase",
    "pattern": "#Subject+",
    "tag": {
      "all": [
        "Subject",
        "Final"
      ]
    }
  },
  "Direct w/ Indirect Objects 1": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "7",
    "type": "intra-phrase",
    "pattern": "#Verbs+ #Agent+ #Nominal+",
    "tag": {
      "on": {
        "term": "(#Nominal+ && !#Agent)",
        "termTag": [
          "DirectObject",
          "DirectObject"
        ]
      },
      "all": "#Predicate"
    },
    "modifier": {
      "on": {
        "term": "(#Nominal+ && !#Agent)",
        "modifies": "#Verbs+",
        "termTag": "DirectObject"
      }
    }
  },
  "Indirect Objects 1": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "8",
    "type": "intra-phrase",
    "pattern": "#Verbs+ #Agent+ #DirectObject",
    "tag": {
      "on": {
        "term": "#Agent+",
        "termTag": [
          "IndirectObject",
          "IndirectObject"
        ]
      },
      "all": "#Predicate"
    },
    "modifier": {
      "on": {
        "term": "#Agent+",
        "modifies": "#Verbs+",
        "termTag": "IndirectObject"
      }
    }
  },
  "Direct w/ Indirect Objects 2": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "9",
    "type": "intra-phrase",
    "pattern": "#Verbs+ #Nominal+ (to|for) #Agent+",
    "tag": {
      "on": {
        "term": "(#Nominal+ && !#Agent)",
        "termTag": [
          "DirectObject",
          "DirectObject"
        ]
      },
      "all": "#Predicate"
    },
    "modifier": {
      "on": {
        "term": "(#Nominal+ && !#Agent)",
        "modifies": "#Verbs+",
        "termTag": "DirectObject"
      }
    }
  },
  "Indirect Objects 2": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "10",
    "type": "intra-phrase",
    "pattern": "#Verbs+ #DirectObject+ (to|for) #Agent+",
    "tag": {
      "on": {
        "term": "#Agent+",
        "termTag": [
          "IndirectObject",
          "IndirectObject"
        ]
      },
      "all": "#Predicate"
    },
    "modifier": {
      "on": {
        "term": "#Agent+",
        "modifies": "#Verbs+",
        "termTag": "IndirectObject"
      }
    }
  },
  "Direct Object": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "11",
    "type": "intra-phrase",
    "pattern": "#Verbs+ #Nominal+",
    "tag": {
      "on": {
        "term": "#Nominal+",
        "termTag": "DirectObject"
      },
      "all": "#Predicate"
    },
    "modifier": {
      "on": {
        "term": "#Nominal+",
        "modifies": "#Verbs+",
        "termTag": "DirectObject"
      }
    }
  },
  "Inter-Phrased Subject Predicate": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "12",
    "type": "inter-phrase",
    "pattern": "#Subject+ #Verbs+ (#DirectObject+|#DirectObject+ #IndirectObject+|#IndirectObject+ #DirectObject+)? #AdverbialPhrase?+",
    "tag": {
      "after": {
        "term": "#Subject+",
        "termTag": [
          "#Predicate",
          "Final"
        ]
      }
    }
  },
  "Clause": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "13",
    "type": "intra-phrase",
    "pattern": "#Subject+ #Predicate+",
    "tag": {
      "all": [
        "Clause",
        "Final"
      ]
    }
  },
  "Compound Sentence": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "13",
    "type": "inter-phrase",
    "pattern": "* #Clause+ *  #CoordinatingConjunction * #Clause+ *",
    "tag": {
      "all": [
        "#CompoundSentence",
        "Final"
      ]
    }
  },
  "Complete Sentence 1": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "15",
    "type": "inter-phrase",
    "pattern": "#Clause+",
    "tag": {
      "all": [
        "CompleteSentence",
        "Final"
      ]
    },
    "untag": {
      "all": "#Clause"
    }
  },
  "Complete Sentence 2": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "16",
    "type": "inter-phrase",
    "pattern": "#Final?+ #Clause+ #Final?+",
    "tag": {
      "all": [
        "CompleteSentence",
        "Final"
      ]
    }
  },
  "Complete Sentence 3": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "17",
    "type": "inter-phrase",
    "pattern": "* #Clause+",
    "tag": {
      "all": [
        "CompleteSentence",
        "Final"
      ]
    }
  },
  "Complete Sentence 4": {
    "batch": "Sub-Structure",
    "batchOrder": "13",
    "order": "18",
    "type": "inter-phrase",
    "pattern": "#Clause+ *",
    "tag": {
      "all": [
        "CompleteSentence",
        "Final"
      ]
    }
  }
}