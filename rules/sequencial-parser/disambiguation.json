{
  "#BaseVerb is #Noun": {
    "batch": "Disambiguation",
    "batchOrder": "1",
    "order": "1",
    "pattern": "to #BaseVerb #BaseVerb",
    "replace": {
      "ending": {
        "termTag": "Noun",
        "termUnTag": "#BaseVerb"
      }
    }
  },
  "Potential Stative Verbs 1": {
    "batch": "Disambiguation",
    "batchOrder": "1",
    "order": "2",
    "pattern": "#PotentialStative #Determiner",
    "replace": {
      "on": {
        "term": "#PotentialStative",
        "termTag": "#Action",
        "termUnTag": "#PotentialStative"
      }
    }
  },
  "Potential Stative Verbs 2": {
    "batch": "Disambiguation",
    "batchOrder": "1",
    "order": "3",
    "pattern": "#PotentialStative (#Where|#When)",
    "replace": {
      "on": {
        "term": "#PotentialStative",
        "termTag": "#Action",
        "termUnTag": "#PotentialStative"
      }
    }
  },
  "Potential Stative Verbs 3": {
    "batch": "Disambiguation",
    "batchOrder": "1",
    "order": "4",
    "pattern": "#PotentialStative (like|of)",
    "replace": {
      "on": {
        "term": "#PotentialStative",
        "termTag": "#Stative",
        "termUnTag": "#PotentialStative"
      }
    }
  }
}