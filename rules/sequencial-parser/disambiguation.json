{
  "#BaseVerb is #Noun": {
    "batch": "Disambiguation",
    "batchOrder": "1",
    "order": "1",
    "pattern": "to #BaseVerb #BaseVerb",
    "replace": {
      "ending": {
        "termTag": "Noun",
        "termUnTag": "#BaseVerb"
      }
    }
  },
  "Potential Linking Verbs 1": {
    "batch": "Disambiguation",
    "batchOrder": "1",
    "order": "2",
    "pattern": "#PotentialLinking #Determiner",
    "replace": {
      "on": {
        "term": "#PotentialLinking",
        "termTag": "#Action",
        "termUnTag": "#PotentialLinking"
      }
    }
  },
  "Potential Linking Verbs 2": {
    "batch": "Disambiguation",
    "batchOrder": "1",
    "order": "3",
    "pattern": "#PotentialLinking (#Where|#When)",
    "replace": {
      "on": {
        "term": "#PotentialLinking",
        "termTag": "#Action",
        "termUnTag": "#PotentialLinking"
      }
    }
  },
  "Potential Linking Verbs 3": {
    "batch": "Disambiguation",
    "batchOrder": "1",
    "order": "4",
    "pattern": "#PotentialLinking (like|of)",
    "replace": {
      "on": {
        "term": "#PotentialLinking",
        "termTag": "#Linking",
        "termUnTag": "#PotentialLinking"
      }
    }
  }
}