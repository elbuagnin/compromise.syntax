{
    "No Comma Adjective": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "4",  "order": "1", "type": "intra-phrase", "pattern": "(#Adjective && !#Comma)", "tag": {"all": "NoCommaAdjective"}},

    "Compound Adjective Phrases": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "4",  "order": "1", "type": "intra-phrase", "pattern": "#NoCommaAdjective #CoordinatingConjunction #Adjective", "tag": {"all": ["CompoundPhrase", "CompoundAdjectives", "aj"]}},

    "Coordinate Adjectives 1": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "4",  "order": "2", "type": "inter-phrase", "pattern": "#Adjective #CommaAdjective+ #NoCommaAdjective", "tag": {"all": ["CoordinatingAdjectives", "aj"]}},

    "Coordinate Adjectives 2": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "4",  "order": "3", "type": "inter-phrase", "pattern": "#Adjective #CommaAdjective+ (and|but) #NoCommaAdjective", "tag": {"all": ["CoordinatingAdjectives", "aj"]}},

    "Cumulative Adjectives": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "4",  "order": "4", "type": "inter-phrase", "pattern": "(#NoCommaAdjective && !#CoordinatingAdjectives && !#CompoundPhrase){2,}", "tag": {"all": ["CumulativeAdjectives", "aj"]}},

    "Noun Lists 1": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "4",  "order": "5", "type": "inter-phrase", "pattern": "#CommaNoun+ #CoordinatingConjunction #Noun+", "tag": {"all": ["List", "nn"]}},

    "Noun Lists 2": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "4",  "order": "7", "type": "inter-phrase", "pattern": "(#Adjective?+ #CommaNoun+)+ #CoordinatingConjunction #Adjective?+ #Noun+", "tag": {"all": ["List", "nn"]}, "untag": {"on": {"term": "#aj", "termUnTag": "#aj"}}},

    "Compound Noun Phrases 1": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "4",  "order": "11", "type": "intra-phrase", "pattern": "(#Noun+ && !#List && !#Comma) #CoordinatingConjunction #Noun+", "tag": {"all": ["CompoundPhrase", "CompoundNouns", "nn"]}, "untag": {"on": {"term": "#aj", "termUnTag": "#aj"}}},

    "Compound Noun Phrases 2": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "4",  "order": "12", "type": "intra-phrase", "pattern": "#Adjective?+ (#Noun+ && !#List && !#Comma) #CoordinatingConjunction #Adjective?+ #Noun+", "tag": {"all": ["CompoundPhrase", "CompoundNouns", "nn"]}, "untag": {"on": {"term": "#aj", "termUnTag": "#aj"}}},

    "Compound Verb Phrases": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "4",  "order": "13", "type": "intra-phrase", "pattern": "#Verb #CoordinatingConjunction #Verb", "tag": {"all": ["CompoundPhrase", "CompoundVerbs", "vb"]}}

}
