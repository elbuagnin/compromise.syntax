{
  "Double Relative Clause": {
    "batch": "Relative Clauses",
    "batchOrder": "9",
    "order": "1",
    "type": "inter-phrase",
    "pattern": "!#Nominal !#Verbs !#Nominal? #RelativePronoun+ .{1,} #RelativePronoun+",
    "replace": {
      "first": {
        "term": "#RelativePronoun+",
        "termUnTag": [
          "#RelativePronoun+",
          "#Restrictive",
          "NonRestrictive"
        ],
        "termTag": [
          "Determiner",
          "Determiner"
        ]
      }
    }
  },
  "Relative Clause 1": {
    "batch": "Relative Clauses",
    "batchOrder": "9",
    "order": "2",
    "type": "intra-phrase",
    "pattern": "#RelativePronoun+ #Nominal+ #Verbs+ #Nominal+",
    "tag": {
      "all": "RelativeClause"
    },
    "untag": {
      "all": [
        "#Nominal",
        "#Verbs"
      ]
    }
  },
