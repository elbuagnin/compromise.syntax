{
  "Subject/Verb 1": {"batch":"Structure", "batchOrder": "13", "order": "1", "type": "intra-phrase", "pattern": "#nn+ * #vb+", "tag": {"before": {"term": "#vb+", "termTag": ["Subject", "sbj"]}}},

  "Subject/Verb 2": {"batch":"Structure", "batchOrder": "13", "order": "2", "type": "intra-phrase", "pattern": "#sbj+ #vb+ *", "tag": {"after": {"term": "#sbj+", "termTag": ["Predicate", "pred"]}}},

  "Subject/Verb 3": {"batch":"Structure", "batchOrder": "13", "order": "3", "type": "intra-phrase", "pattern": "#sbj+ #vb+ *", "tag": {"after": {"term": "#sbj+", "termTag": ["Predicate", "pred"]}}},

  "Noun Subject Compliment": {"batch":"Structure", "batchOrder": "13", "order": "4", "type": "intra-phrase", "pattern": "#sbj+ #Linking+ #nn+", "tag": {"on": {"term": "#nn+", "termTag": "SubjectComplement"}}, "modifier": {"on": {"term": "#nn+", "modifies": "#sbj+", "termTag": "SubjectComplement"}}},

  "Adjective Subject Compliment": {"batch":"Structure", "batchOrder": "13", "order": "5", "type": "intra-phrase", "pattern": "#sbj+ #Linking+ #aj+", "tag": {"on": {"term": "#aj+", "termTag": "SubjectComplement"}}, "modifier": {"on": {"term": "#aj+", "modifies": "#sbj+", "termTag": "SubjectComplement"}}},

  "Subject": {"batch":"Structure", "batchOrder": "13", "order": "6", "type": "inter-phrase", "pattern": "#sbj+", "tag": {"all": ["Subject", "Final"]}},

  "Direct w/ Indirect Objects 1": {"batch":"Structure", "batchOrder": "13", "order": "7", "type": "intra-phrase", "pattern": "#vb+ #Agent+ #nn+", "tag": {"on": {"term": "(#nn+ && !#Agent)", "termTag": ["DirectObject", "dobj"]}}, "modifier": {"on": {"term": "(#nn+ && !#Agent)", "modifies": "#vb+", "termTag": "DirectObject"}}},

  "Indirect Objects 1": {"batch":"Structure", "batchOrder": "13", "order": "8", "type": "intra-phrase", "pattern": "#vb+ #Agent+ #dobj", "tag": {"on": {"term": "#Agent+", "termTag": ["IndirectObject", "iobj"]}}, "modifier": {"on": {"term": "#Agent+", "modifies": "#vb+", "termTag": "IndirectObject"}}},

  "Direct w/ Indirect Objects 2": {"batch":"Structure", "batchOrder": "13", "order": "9", "type": "intra-phrase", "pattern": "#vb+ #nn+ (to|for) #Agent+", "tag": {"on": {"term": "(#nn+ && !#Agent)", "termTag": ["DirectObject", "dobj"]}}, "modifier": {"on": {"term": "(#nn+ && !#Agent)", "modifies": "#vb+", "termTag": "DirectObject"}}},

  "Indirect Objects 2": {"batch":"Structure", "batchOrder": "13", "order": "10", "type": "intra-phrase", "pattern": "#vb+ #dobj+ (to|for) #Agent+", "tag": {"on": {"term": "#Agent+", "termTag": ["IndirectObject", "iobj"]}}, "modifier": {"on": {"term": "#Agent+", "modifies": "#vb+", "termTag": "IndirectObject"}}},

  "Direct Object": {"batch":"Structure", "batchOrder": "13", "order": "11", "type": "intra-phrase", "pattern": "#vb+ #nn+", "tag": {"on": {"term": "#nn+", "termTag": "DirectObject"}}, "modifier": {"on": {"term": "#nn+", "modifies": "#vb+", "termTag": "DirectObject"}}},

  "Inter-Phrased Subject Predicate": {"batch":"Structure", "batchOrder": "13", "order": "12", "type": "inter-phrase", "pattern": "#Subject+ #vb+ (#DirectObject+|#DirectObject+ #IndirectObject+|#IndirectObject+ #DirectObject+)? #AdverbialPhrase?+", "tag": { "after": {"term": "#Subject+", "termTag": ["#Predicate", "Final"]}}},

  "Clause": {"batch":"Structure", "batchOrder": "13", "order": "13", "type": "intra-phrase", "pattern": "#Subject+ #Predicate+", "tag": {"all": ["Clause", "Final"]}},

  "Compound Sentence": {"batch":"Structure", "batchOrder": "13", "order": "14", "type": "inter-phrase", "pattern": "* #Clause+ *  #CoordinatingConjunction * #Clause+ *", "tag": {"all": ["#CompoundSentence", "Final"]}},

  "Complete Sentence 1": {"batch":"Structure", "batchOrder": "13", "order": "15", "type": "inter-phrase", "pattern": "#Clause+", "tag": {"all": ["CompleteSentence", "Final"]}, "untag": {"all": "#Clause"}},

  "Complete Sentence 2": {"batch":"Structure", "batchOrder": "13", "order": "16", "type": "inter-phrase", "pattern": "#Final?+ #Clause+ #Final?+", "tag": {"all": ["CompleteSentence", "Final"]}},

  "Complete Sentence 3": {"batch":"Structure", "batchOrder": "13", "order": "17", "type": "inter-phrase", "pattern": "* #Clause+", "tag": {"all": ["CompleteSentence", "Final"]}},

  "Complete Sentence 4": {"batch":"Structure", "batchOrder": "13", "order": "18", "type": "inter-phrase", "pattern": "#Clause+ *", "tag": {"all": ["CompleteSentence", "Final"]}}
}
