{
    "Adjective Phrases": {"batch":"Phrases, 1st Pass", "batchOrder": "5",  "order": "1", "type": "intra-phrase", "pattern": "#av+ #aj+", "tag": {"all": ["AdjectivePhrase", "aj"]}, "modifier": {"term": "#av+", "modifies": "#aj+"}},

    "Gerund Phrase": {"batch":"Phrases, 1st Pass", "batchOrder": "5",  "order": "2", "type": "intra-phrase", "pattern": "#Gerund (#Determiner #Noun|#Plural)", "tag": {"all": ["GerundPhrase", "nn"]}, "modifier": {"term": "#noun+", "modifies": "#Gerund", "termTag": "#ObjectOfGerund"}},

    "Noun Phrases 1": {"batch":"Phrases, 1st Pass", "batchOrder": "5",  "order": "3", "type": "intra-phrase", "pattern": "#aj+ #nn+","tag": {"all": ["NounPhrase", "nn"]}, "untag": {"all": "#aj"}, "modifier": {"term": "#aj+", "modifies": "#nn+"}},

    "Noun Phrases 2": {"batch":"Phrases, 1st Pass", "batchOrder": "5",  "order": "4", "type": "intra-phrase", "pattern": "#dt #nn+", "tag": {"all": ["NounPhrase", "nn"]}},

    "Head Noun": {"batch":"Phrases, 1st Pass", "batchOrder": "5",  "order": "5", "type": "intra-phrase", "pattern": "#NounPhrase+", "tag": {"on": {"term": "#Noun", "termTag": "HeadNoun"}}},

    "Simple Relative Noun Clauses": {"batch": "Phrases, 1st Pass", "batchOrder": "5", "order": "6", "type": "intra-phrase", "pattern": "#QuestionWord #Pronoun #Past", "tag": {"all": ["RelativeClause", "nn"], "on": {"term": "#QuestionWord", "termTag": "RelativePronoun"}}, "untag": {"on": {"term": "#QuestionWord", "termUnTag": "#QuestionWord"}}},

    "Prepositional Phrases 1": {"batch": "Phrases, 1st Pass", "batchOrder": "5", "order": "7", "type": "intra-phrase", "pattern": "#Preposition #aj?+ #nn+", "tag": {"all": "PrepositionalPhrase"}, "modifier": {"term": "#nn+", "modifies": "#Preposition", "termTag": "#ObjectOfPreposition"}},

    "Prepositional Phrase with Gerund as Object 2": {"batch":"Phrases, 1st Pass", "batchOrder": "5",  "order": "8", "type": "intra-phrase", "pattern": "#Preposition #Gerund #nn+", "tag": {"all": "PrepositionalPhrase"}, "modifier": {"term": "#GerundPhrase+", "modifies": "#Preposition", "termTag": "#ObjectOfPreposition"}}
}
