{
      "Prepostional Phrases": {"batch":"Simple Phrases, 2nd Pass", "batchOrder": "3",  "order": "1", "type": "intra-phrase", "pattern": "#Preposition (#NounPhrase?+|#Noun|#Pronoun)", "tag": {"all": "PrepositionalPhrase"}},

      "Adverb Verb Phrases": {"batch":"Simple Phrases, 2nd Pass", "batchOrder": "3",  "order": "2", "type": "intra-phrase", "pattern": "(#Adverb?+|#Adverb?)? #Verb{2,} (#Adverb?+|#Adverb?)?", "tag": {"all": "VerbPhrase"}},

      "VerbalPhrase": {"batch": "Simple Phrases, 2nd Pass", "batchOrder": "3", "order": "3", "type": "intra-phrase", "pattern": "#Gerund (#PrepositionalPhrase+|#NounPhrase+)", "tag": {"all": "VerbalPhrase"}},

      "Perfect Participle Phrase": {"batch":"Simple Phrases, 2nd Pass", "batchOrder": "3",  "order": "4", "type": "intra-phrase", "pattern": "#PerfectParticiple+ #NounPhrase+", "tag": {"all": "ParticiplePhrase"}},

      "Not to": {"batch":"Simple Phrases, 2nd Pass", "batchOrder": "3",  "order": "5", "type": "intra-phrase", "pattern": "not to #Infinitive", "tag": {"all": ["Infinitive", "Negative"]}}
}
