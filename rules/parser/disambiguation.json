{
  "#FiniteVerb is #Noun": {"batch":"Disambiguation", "batchOrder": "1",  "order": "1", "pattern": "to #FiniteVerb #FiniteVerb", "replace": {"ending": {"termTag": "Noun", "termUnTag": "FiniteVerb"}}},

  "Do not": {"batch":"Disambiguation", "batchOrder": "1",  "order": "2", "type": "intra-phrase", "pattern": "(do|did|does) not !#Verb", "tag": {"all": ["Negative", "Implied", "VerbPhrase", "Verbial"]}},

  "Do not #FiniteVerb": {"batch":"Disambiguation", "batchOrder": "1",  "order": "3", "type": "intra-phrase", "pattern": "(do|did|does) not #FiniteVerb", "tag": {"all": ["Negative", "VerbPhrase", "Verbial"], "eachTheSame": {"terms": ["do", "did", "does"], "termTag": "Auxiliary"}}},

  "#Modal not": {"batch":"Disambiguation", "batchOrder": "1",  "order": "4", "type": "intra-phrase", "pattern": "#Modal not !#Verb", "tag": {"all": ["Negative", "Implied", "VerbPhrase", "Verbial"]}},

  "#Modal not #FiniteVerb": {"batch":"Disambiguation", "batchOrder": "1",  "order": "4", "type": "intra-phrase", "pattern": "#Modal not #FiniteVerb", "tag": {"all": ["Negative", "VerbPhrase", "Verbial"]}},

  "never #PresentTense": {"batch":"Disambiguation", "batchOrder": "1",  "order": "4", "type": "intra-phrase", "pattern": "#Modal? never #PresentTense", "tag": {"all": ["Negative", "VerbPhrase", "Verbial"]}},

  "Not to": {"batch":"Disambiguation", "batchOrder": "1",  "order": "5", "type": "intra-phrase", "pattern": "not to #Infinitive", "tag": {"all": ["Infinitive", "Negative"]}},

  "Not Gerund/Present Pariciple": {"batch":"Disambiguation", "batchOrder": "1",  "order": "5", "type": "intra-phrase", "pattern": "(not && !#Verbial) (#Verbal && /ing$/)", "tag": {"all": ["Negative", "VerbalPhrase"]}},

  "Infinitive Phrase 1": {"batch":"Disambiguation", "batchOrder": "1",  "order": "6", "type": "intra-phrase", "pattern": "#Infinitive+ (#Noun|#Preposition)", "tag": {"all": "InfinitivePhrase"}}
}
