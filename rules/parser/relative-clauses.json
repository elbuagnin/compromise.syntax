{
  "Double Relative Clause": {"batch":"Phrases, 3rd Pass", "batchOrder": "12",  "order": "1", "type": "inter-phrase", "pattern": "#RelativePronoun *  #RelativePronoun", "replace": {"first": {"term": "#RelativePronoun", "termUnTag": ["#RelativePronoun", "#Restrictive", "NonRestrictive"], "termTag": "Determiner"}}},

  "Relative Clause 1": {"batch":"Phrases, 3rd Pass", "batchOrder": "12",  "order": "2", "type": "intra-phrase", "pattern": "#RelativePronoun #Verbial+ (#Adjectival+|#Nominal+)", "tag": {"all": ["RelativeClause", "Adjectival"]}, "untag": {"all": ["#Nominal", "#Verbial"]}},

  "Relative Clause 2": {"batch":"Phrases, 3rd Pass", "batchOrder": "12",  "order": "3", "type": "intra-phrase", "pattern": "#RelativePronoun #Verbial+ Nominal+", "tag": {"all": ["RelativeClause", "Adjectival"]}, "untag": {"all": ["#Nominal", "#Verbial"]}},

  "Relative Clause 5": {"batch":"Phrases, 3rd Pass", "batchOrder": "12",  "order": "4", "type": "intra-phrase", "pattern": "#RelativePronoun #Verbial+ #PrepositionalPhrase+", "tag": {"all": ["RelativeClause", "Adjectival"]}, "untag": {"all": ["#Nominal", "#Verbial"]}},

  "Relative Clause 6": {"batch":"Phrases, 3rd Pass", "batchOrder": "12",  "order": "5", "type": "intra-phrase", "pattern": "#RelativePronoun (#Pronoun|#Person) #Verbial+ (#Adverb?|#Preposition?|#Date?)", "tag": {"all": ["RelativeClause", "Adjectival"]}, "untag": {"all": ["#Nominal", "#Verbial"]}},

  "Relative Clause 7": {"batch":"Phrases, 3rd Pass", "batchOrder": "12",  "order": "6", "type": "intra-phrase", "pattern": "#RelativePronoun (#Pronoun|#Person) #Verbial+ #Nominal+ #Verbial?+ #Preposition?", "tag": {"all": ["RelativeClause", "Adjectival"]}, "untag": {"all": ["#Nominal", "#Verbial"]}},

  "Relative Clause 8": {"batch":"Phrases, 3rd Pass", "batchOrder": "12",  "order": "7", "type": "intra-phrase", "pattern": "#RelativePronoun #Verbial+ #VerbalPhrase+", "tag": {"all": ["RelativeClause", "Adjectival"]}, "untag": {"all": ["#Nominal", "#Verbial"]}}
}
