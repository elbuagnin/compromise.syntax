{
  "#BaseVerb is #Noun": {"batch":"Disambiguation & Early Groupings", "batchOrder": "1",  "order": "1", "pattern": "to #BaseVerb #BaseVerb", "replace": {"ending": {"termTag": "Noun", "termUnTag": "BaseVerb"}}},

  "Not to": {"batch":"Disambiguation & Early Groupings", "batchOrder": "1",  "order": "2", "type": "intra-phrase", "pattern": "not to #Infinitive", "tag": {"all": ["Infinitive", "Negative"]}},

  "Nouns": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "8", "type": "inter-phrase", "pattern": "#Noun", "tag": {"all": "nn"}},

  "Verbs": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "8", "type": "inter-phrase", "pattern": "#Verb", "tag": {"all": "vb"}},

  "Adjectives": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "8", "type": "inter-phrase", "pattern": "#Adjective", "tag": {"all": "ad"}},

  "Adverbs": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "8", "type": "inter-phrase", "pattern": "#Noun", "tag": {"all": "av"}},

  "Enities 1": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "4", "type": "inter-phrase", "pattern": "(#Person|#Determiner #Noun)", "tag": {"all": ["Entity", "nn"]}},

  "Enities 2": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "5", "type": "intra-phrase", "pattern": "(#Pronoun && !#RelativePronoun)", "tag": {"all": ["Entity", "nn"]}},

  "Proper Noun String": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "6", "type": "intra-phrase", "pattern": "#ProperNoun{2,}", "tag": {"all": ["Entity", "Nouns", "nn"]}},

  "Noun String": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "7", "type": "intra-phrase", "pattern": "#Noun{2,}", "tag": {"all": ["Nouns", "nn"]}},

  "Adverb String": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "8", "type": "intra-phrase", "pattern": "#Adverb{2,}", "tag": {"all": ["Adverbs", "av"]}}
}
