{
  "#BaseVerb is #Noun": {"batch":"Disambiguation & Early Groupings", "batchOrder": "1",  "order": "1", "pattern": "to #BaseVerb #BaseVerb", "replace": {"ending": {"termTag": "Noun", "termUnTag": "BaseVerb"}}},

  "Not to": {"batch":"Disambiguation & Early Groupings", "batchOrder": "1",  "order": "2", "type": "intra-phrase", "pattern": "not to #Infinitive", "tag": {"all": ["Infinitive", "Negative"]}},

  "Nouns": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "3", "type": "inter-phrase", "pattern": "(#Noun|#Pronoun|#Gerund)", "tag": {"all": "nn"}},

  "Verbs": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "4", "type": "inter-phrase", "pattern": "#Verb", "tag": {"all": "vb"}},

  "Adjectives": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "5", "type": "inter-phrase", "pattern": "#Adjective", "tag": {"all": "aj"}},

  "Adverbs": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "6", "type": "inter-phrase", "pattern": "#Adverb", "tag": {"all": "av"}},

  "Determiners": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "7", "type": "inter-phrase", "pattern": "(#Determiner|#Possessive)", "tag": {"all": "dt"}},

  "Agents 1": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "8", "type": "inter-phrase", "pattern": "(#Person|#Actor)", "tag": {"all": "Agent"}},

  "Agents 2": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "9", "type": "intra-phrase", "pattern": "(#Pronoun && !#RelativePronoun)", "tag": {"all": "Agent"}},

  "Agents 3": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "10", "type": "inter-phrase", "pattern": "(person|people)", "tag": {"all": "Agent"}},

  "Proper Noun String": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "11", "type": "intra-phrase", "pattern": "#ProperNoun{2,}", "tag": {"all": ["Agent", "Person", "Nouns"]}},

  "Noun String": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "12", "type": "intra-phrase", "pattern": "#Noun{2,}", "tag": {"all": "Nouns"}},

  "Adjective String": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "13", "type": "intra-phrase", "pattern": "#Adjective{2,}", "tag": {"all": "Adjectives"}},

  "Adverb String": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "14", "type": "intra-phrase", "pattern": "#Adverb{2,}", "tag": {"all": "Adverbs"}},

  "Verb String": {"batch": "Disambiguation & Early Groupings", "batchOrder": "1", "order": "15", "type": "intra-phrase", "pattern": "#Verb{2,}", "tag": {"all": "Verbs"}}
}
