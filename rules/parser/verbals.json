{
  "Perfect Participle Active": {"batch": "Verbals", "batchOrder": "3", "order": "1", "type": "intra-phrase", "pattern": "having #PastTense", "tag": {"all": ["PerfectParticiple", "Active"]}, "untag": {"all": ["#Verbs", "#vb"]}},

  "Perfect Participle Passive": {"batch": "Verbals", "batchOrder": "3", "order": "2", "type": "intra-phrase", "pattern": "having been #PastTense", "tag": {"all": ["PerfectParticiple", "Passive"]}, "untag": {"all": ["#Verbs", "#vb"]}},

  "Past Participle 1": {"batch": "Verbals", "batchOrder": "3", "order": "3", "type": "intra-phrase", "pattern": "#Participle", "tag": {"on": {"term": "(#Participle && !#PerfectParticiple)", "termTag": "#PastParticiple"}}},

  "Past Participle 2": {"batch": "Verbals", "batchOrder": "3", "order": "4", "type": "intra-phrase", "pattern": "^(#PastTense && !#PerfectParticiple) (* && !#vb) #Comma", "tag": {"on": {"term": "#PastTense", "termTag": "#PastParticiple"}}, "untag": {"all": "vb"}},

  "Past Participle 3": {"batch": "Verbals", "batchOrder": "3", "order": "5", "type": "intra-phrase", "pattern": "^(#PastTense && #Comma && !#PerfectParticiple)", "tag": {"on": {"term": "#PastTense", "termTag": ["#PastParticiple", "aj"]}}, "untag": {"on": {"term": "#PastParticiple", "termUnTag": ["#Verbs", "#vb"]}}},

  "Present Participle 1": {"batch": "Verbals", "batchOrder": "3", "order": "6", "type": "intra-phrase", "pattern": "^(#Verbal && /ing$/ && #Comma)", "tag": {"on": {"term": "#Verbal", "termTag": ["#PresentParticiple", "aj"]}}},

  "Present Participle 2": {"batch": "Verbals", "batchOrder": "3", "order": "7", "type": "intra-phrase", "pattern": "^(#Verbal && /ing$/ (* && !#vb) #Comma)", "tag": {"on": {"term": "#Verbal", "termTag": "#PresentParticiple"}}},

  "Present Participle 3": {"batch": "Verbals", "batchOrder": "3", "order": "8", "type": "intra-phrase", "pattern": "#dt (#Verbal && /ing$/) #nn+", "tag": {"on": {"term": "#Verbal", "termTag": "#PresentParticiple"}}},

  "Present Participle 4": {"batch": "Verbals", "batchOrder": "3", "order": "9", "type": "intra-phrase", "pattern": "#nn+ (#Verbal && /ing$/) #nn+", "tag": {"on": {"term": "#Verbal", "termTag": "#PresentParticiple"}}},

  "Present Participle 5": {"batch": "Verbals", "batchOrder": "3", "order": "10", "type": "intra-phrase", "pattern": "#av+ (#Verbal && /ing$/)", "tag": {"on": {"term": "#Verbal", "termTag": "#PresentParticiple"}}},

  "Present Participle 6": {"batch": "Verbals", "batchOrder": "3", "order": "11", "type": "inter-phrase", "pattern": "#nn+ (#Verbal && /ing$/)", "tag": {"on": {"term": "#Verbal", "termTag": "#PresentParticiple"}}},

  "Gerund 1": {"batch":"Verbals", "batchOrder": "3",  "order": "12", "type": "intra-phrase", "pattern": "(#Verbal && !#Participle && /ing$/) #vb+", "tag": {"on": {"term": "#Verbal", "termTag": ["Gerund", "gd", "nn"]}}},

  "Gerund 2": {"batch":"Verbals", "batchOrder": "3",  "order": "13", "type": "intra-phrase", "pattern": "#aj+ (#Verbal && !#Participle && /ing$/)", "tag": {"on": {"term": "#Verbal", "termTag": ["Gerund", "gd", "nn"]}}},

  "Gerund 3": {"batch":"Verbals", "batchOrder": "3",  "order": "14", "type": "intra-phrase", "pattern": "#Preposition+ (#Verbal && !#Participle && /ing$/)", "tag": {"on": {"term": "#Verbal", "termTag": ["Gerund", "gd", "nn"]}}},

  "Gerund 4": {"batch":"Verbals", "batchOrder": "3",  "order": "15", "type": "intra-phrase", "pattern": "(#Verbal && !#Participle && /ing$/)", "tag": {"on": {"term": "#Verbal", "termTag": ["Gerund", "gd", "nn"]}}},

  "Infinitive Phrase": {"batch":"Phrases, 2nd Pass", "batchOrder": "3",  "order": "16", "type": "intra-phrase", "pattern": "#Infinitive+ #nn+", "tag": {"all": ["InfinitivePhrase", "iv"]}, "untag": {"all": "#nn"}, "modifier": {"term": "#nn+", "modifies": "#Infinitive+", "termTag": "#ObjectOfInfinitive"}},

  "Tag #vl": {"batch": "Verbals", "batchOrder": "3", "order": "17", "type": "intra-phrase", "pattern": "#Verbal", "tag": {"all": "vl"}},

  "Uncaught Verbs 1": {"batch":"Verbs", "batchOrder": "3",  "order": "18", "type": "inter-phrase", "pattern": "#PastTense", "tag": {"on": {"term": "#PastTense", "termTag": "PastSimple"}}},

  "Uncaught Verbs 2": {"batch":"Verbs", "batchOrder": "3",  "order": "19", "type": "inter-phrase", "pattern": "#PresentTense", "tag": {"on": {"term": "#PresentTense", "termTag": "PresentSimple"}}}
}
