{
    "Simple Lists": {"batch":"Simple Phrases, 1st Pass", "batchOrder": "1",  "order": "1", "pattern": "#Comma+ #Comma", "tag": {"all": "List"}},

    "Noun Phrases": {"batch":"Simple Phrases, 1st Pass", "batchOrder": "1",  "order": "2", "pattern": "(#Determiner?|#Possessive?) (#Adjective?+|#Adjective?)? #Noun?+ #Noun", "tag": {"all": "NounPhrase"}, "on": {"Noun": "HeadNoun"}},

    "Linking Verb Phrases": {"batch":"Simple Phrases, 1st Pass", "batchOrder": "1", "order": "3", "pattern": "#AuxiliaryVerbs? #Verb", "tag": {"all": "LinkingVerbPhrase"}}    
}
