{
    "Simple Lists": {"batch":"Simple Phrases, 1st Pass", "batchOrder": "2",  "order": "1", "type": "intra-phrase", "pattern": "#Comma+ #Comma", "tag": {"all": "List"}},

    "Adjective Phrases 1": {"batch":"Simple Phrases, 1st Pass", "batchOrder": "2",  "order": "2", "type": "intra-phrase", "pattern": "#Determiner? #Adverb?+ #Adjective?+ #Adjective #Adjective", "tag": {"all": "AdjectivePhrase", "last": {"term": "#Adjective", "termTag": "HeadAdjective"}}},

    "Adjective Phrases 2": {"batch":"Simple Phrases, 1st Pass", "batchOrder": "2",  "order": "3", "type": "intra-phrase", "pattern": "#Determiner? #Adverb?+ #Adverb #Adjective?+ #Adjective", "tag": {"all": "AdjectivePhrase", "last": {"term": "#Adjective", "termTag": "HeadAdjective"}}},

    "Noun Phrases 1": {"batch":"Simple Phrases, 1st Pass", "batchOrder": "2",  "order": "4", "type": "intra-phrase", "pattern": "(#Determiner?|#Possessive?) (#AdjectivePhrase?+|#Adjective?)? #Noun?+ #Noun", "tag": {"all": "NounPhrase"}, "on": {"Noun": "HeadNoun"}},

    "Noun Phrases 2": {"batch":"Simple Phrases, 1st Pass", "batchOrder": "2",  "order": "5", "type": "intra-phrase", "pattern": "(#Determiner?|#Possessive?)? (#AdjectivePhrase?+|#Adjective?) #Noun?+ #Noun", "tag": {"all": "NounPhrase"}, "on": {"Noun": "HeadNoun"}},

    "Infinitive Phrase": {"batch":"Simple Phrases, 1st Pass", "batchOrder": "2",  "order": "6", "type": "intra-phrase", "pattern": "to #Infinitive (#NounPhrase+|#Noun)", "tag": {"all": "InfinitivePhrase"}}
}
