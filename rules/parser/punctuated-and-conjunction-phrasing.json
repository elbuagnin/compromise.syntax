{
    "Simple Noun Lists": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "2",  "order": "1", "type": "inter-phrase", "pattern": "#Noun #Comma+ #Comma and #Noun", "tag": {"all": "List"}},

    "Compound Adjective Phrases": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "2",  "order": "2", "type": "intra-phrase", "pattern": "(#Adjective && !#Comma) #CoordinatingConjunction #Adjective", "tag": {"all": ["CompoundPhrase", "CompoundAdjectives"]}},

    "Coordinate Adjectives 1": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "2",  "order": "3", "type": "inter-phrase", "pattern": "#Adjective (#Adjective && #Comma)+ (#Adjective && !#Comma)", "tag": {"all": "CoordinatingAdjectives"}},

    "Coordinate Adjectives 2": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "2",  "order": "4", "type": "inter-phrase", "pattern": "#Adjective (#Adjective && #Comma)+ (and|but) (#Adjective && !#Comma)", "tag": {"all": "CoordinatingAdjectives"}},

    "Cumulative Adjectives": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "2",  "order": "5", "type": "inter-phrase", "pattern": "(#Adjective && !#CoordinatingAdjectives && !#CompoundPhrase && !#Comma)+", "tag": {"all": "CumulativeAdjectives"}},

    "Compound Sentence": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "2", "order": "6", "type": "inter-phrase", "pattern": "* #Comma #CoordinatingConjunction *", "tag": {"on": {"term": "#CoordinatingConjunction", "termTag": "#CompoundConjunction"}}},

    "Compound Noun Phrases": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "2",  "order": "7", "type": "intra-phrase", "pattern": "(#Noun && !#List && !Comma) #CoordinatingConjunction #Noun", "tag": {"all": ["CompoundPhrase", "CompoundNouns"]}},

    "Compound Verb Phrases": {"batch":"Punctuated and Conjunction Phrasing", "batchOrder": "2",  "order": "8", "type": "intra-phrase", "pattern": "#Verb #CoordinatingConjunction #Verb", "tag": {"all": ["CompoundPhrase", "CompoundVerbs"]}}
}
