{
      "Simple Lists": {"batch":"Simple Phrases", "batchOrder": "1",  "order": "1", "pattern": "#Comma+ #Comma", "tag": {"all": "List"}},

      "Noun Phrases": {"batch":"Simple Phrases", "batchOrder": "3",  "order": "2", "pattern": "(#Determiner?|#Possessive?) (#Adjective?+|#Adjective?)? #Noun?+ #Noun", "tag": {"all": "NounPhrase"}, "on": {"Noun": "HeadNoun"}},

      "Prepostional Phrases": {"batch":"Simple Phrases", "batchOrder": "3",  "order": "3", "pattern": "#Preposition (#NounPhrase?+|#Noun?+ #Noun|#Pronoun)", "tag": {"all": "PrepositionPhrase"}},

      "Gerund Phrases": {"batch":"Simple Phrases", "batchOrder": "3",  "order": "4", "pattern": "#Gerund (#PrepositionPhrase?+|#NounPhrase?+|#Noun?+ #Noun)", "tag": {"all": "GerundPhrase"}},

      "Participle Phrase": {"batch": "Simple Phrases", "batchOrder": "3", "order": "5", "pattern": "", "tag": {"all": "ParticiplePhrase"}},

      "Auxiliary Verb Phrases": {"batch":"Simple Phrases", "batchOrder": "3",  "order": "7", "pattern": "#AuxiliaryVerbs? #Verb", "tag": {"all": "VerbPhrase"}},

      "Adverb Verb Phrases": {"batch":"Simple Phrases", "batchOrder": "3",  "order": "8", "pattern": "(#Adverb?+|#Adverb?)? #Verb{2,} (#Adverb?+|#Adverb?)?", "tag": {"all": "VerbPhrase"}},

      "Not to": {"batch":"Simple Phrases", "batchOrder": "3",  "order": "9", "pattern": "not to #Infinitive", "tag": {"all": ["Infinitive", "Negative"]}}
}
