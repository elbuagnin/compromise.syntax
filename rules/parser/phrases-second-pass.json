{
      "Prepostional Phrases": {"batch":"Phrases, 2nd Pass", "batchOrder": "6",  "order": "1", "type": "intra-phrase", "pattern": "#Preposition (#NounPhrase?+|#Noun|#Pronoun)", "tag": {"all": "PrepositionalPhrase"}},

      "Noun Phrase with Preposition": {"batch":"Phrases, 2nd Pass", "batchOrder": "6",  "order": "2", "type": "intra-phrase", "pattern": "#NounPhrase+ #PrepositionalPhrase+", "tag": {"on": {"term": "#PrepositionalPhrase+", "termTag": ["NounPhrase", "Nominal"]}}},

      "Infinitive Phrase 2": {"batch":"Phrases, 2nd Pass", "batchOrder": "6",  "order": "3", "type": "intra-phrase", "pattern": "#InfinitivePhrase+ (#NounPhrase+|#PrepositionalPhrase+)", "tag": {"all": "InfinitivePhrase"}, "untag": {"all": "#Nominal"}},

      "VerbalPhrase 1": {"batch": "Phrases, 2nd Pass", "batchOrder": "6", "order": "4", "type": "intra-phrase", "pattern": "(#Verbal && !#Infinitive) (#PrepositionalPhrase+|#NounPhrase+)", "tag": {"all": "VerbalPhrase"}, "untag": {"all": "#Nominal"}},

      "Perfect Participle Phrase": {"batch":"Phrases, 2nd Pass", "batchOrder": "6",  "order": "5", "type": "intra-phrase", "pattern": "#PerfectParticiple+ #NounPhrase+", "tag": {"all": ["PerfectParticiplePhrase", "Adjectival"]}, "untag": {"all": "#Nominal"}},

      "VerbalPhrase 2": {"batch":"Phrases, 2nd Pass", "batchOrder": "6",  "order": "6", "type": "intra-phrase", "pattern": "#Verbal #Adverb? #PrepositionalPhrase+", "tag": {"all": "VerbalPhrase"}, "untag": {"all": "#Nominal"}},

      "VerbalPhrase 3": {"batch":"Phrases, 2nd Pass", "batchOrder": "6",  "order": "7", "type": "intra-phrase", "pattern": "#Possessive #Verbal #Pronoun (#NounPhrase+|#Noun?+ #Noun)", "tag": {"all": "VerbalPhrase"}, "untag": {"all": "#Nominal"}},

      "VerbalPhrase 4": {"batch":"Phrases, 2nd Pass", "batchOrder": "6",  "order": "8", "type": "intra-phrase", "pattern": "(#Verbal+ && !#VerbalPhrase)", "tag": {"all": "VerbalPhrase"}}
}
