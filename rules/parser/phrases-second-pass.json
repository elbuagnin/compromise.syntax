      "#Verbal #Adverb": {"batch": "Phrases, 2nd Pass", "batchOrder": "6", "order": "1", "type": "intra-phrase", "pattern": "(#Verbal && !#Infinitive) #av+", "tag": {"all": "vl"}, "modifier": {"term": "#av+", "modifies": "#Verbal+"}},

