{
  "Past Perfect": {"batch": "Verbs", "batchOrder": "3", "order": "1", "type": "intra-phrase", "pattern": "had #PastTense", "tag": {"all": "PastPerfect"}},

  "Past Progressive": {"batch": "Verbs", "batchOrder": "3", "order": "2", "type": "intra-phrase", "pattern": "(was|were) (#Verbal && /ing$/)", "tag": {"all": "PastProgressive"}},

  "Past Perfect Progressive": {"batch": "Verbs", "batchOrder": "3", "order": "3", "type": "intra-phrase", "pattern": "(had|have) been (#Verbal && /ing$/)", "tag": {"all": "PastPerfectProgressive"}},

  "Present Perfect": {"batch": "Verbs", "batchOrder": "3", "order": "4", "type": "intra-phrase", "pattern": "!will (have|has) #PastTense", "tag": {"all": "PresentPerfect"}},

  "Present Progressive": {"batch": "Verbs", "batchOrder": "3", "order": "5", "type": "intra-phrase", "pattern": "(am|is|are) (#Verbal && /ing$/)", "tag": {"all": "PresentProgressive"}},

  "Present Perfect Progressive": {"batch": "Verbs", "batchOrder": "3", "order": "6", "type": "intra-phrase", "pattern": "(has|have) been (#Verbal && /ing$/)", "tag": {"all": "PresentPerfectProgressive"}},

  "Future Simple": {"batch": "Verbs", "batchOrder": "3", "order": "7", "type": "intra-phrase", "pattern": "will #PresentTense", "untag": {"on": {"term": "#PresentTense", "termUnTag": "#PresentTense"}}, "tag": {"all": "#FutureSimple"}},

  "Future Perfect": {"batch": "Verbs", "batchOrder": "3", "order": "8", "type": "intra-phrase", "pattern": "will have #PastTense", "tag": {"all": "FuturePerfect"}},

  "Future Progressive": {"batch": "Verbs", "batchOrder": "3", "order": "9", "type": "intra-phrase", "pattern": "will be (#Verbal && /ing$/)", "tag": {"all": "FutureProgressive"}},

  "Future Perfect Progressive": {"batch": "Verbs", "batchOrder": "3", "order": "10", "type": "intra-phrase", "pattern": "will have been (#Verbal && /ing$/)", "tag": {"all": "FuturePerfectProgressive"}},

  "Past Stative": {"batch": "Verbs", "batchOrder": "3", "order": "11", "type": "intra-phrase", "pattern": "(was|were) !(#Verb|#Verbal)?", "tag": {"on":{"term": "#Verb", "termTag": ["Stative", "Past"]}}},

  "Present Stative": {"batch": "Verbs", "batchOrder": "3", "order": "12", "type": "intra-phrase", "pattern": "(am|is|are) !(#Verb|#Verbal)?", "tag": {"on":{"term": "#Verb", "termTag": ["Stative", "Present"]}}},

  "Future Stative": {"batch": "Verbs", "batchOrder": "3", "order": "13", "type": "intra-phrase", "pattern": "will be !(#Verb|#Verbal)?", "tag": {"on":{"term": "#Verb", "termTag": ["Stative", "Future"]}}},

  "Past Passive": {"batch": "Verbs", "batchOrder": "3", "order": "14", "type": "intra-phrase", "pattern": "(was|were) #PastTense", "tag": {"on":{"term": "#Verb", "termTag": ["Passive", "Past"]}}},

  "Present Passive": {"batch": "Verbs", "batchOrder": "3", "order": "15", "type": "intra-phrase", "pattern": "(am|is|are) #PastTense", "tag": {"on":{"term": "#Verb", "termTag": ["Passive", "Present"]}}},

  "Future Passive": {"batch": "Verbs", "batchOrder": "3", "order": "16", "type": "intra-phrase", "pattern": "will be #PastTense", "tag": {"on":{"term": "#Verb", "termTag": ["Passive", "Future"]}}},

  







  "#Verb #Negative #Verb": {"batch": "Verbs", "batchOrder": "3", "order": "17", "type": "intra-phrase", "pattern": "#Verb #Negative #Verb", "tag": {"all": "#VerbPhrase"}},

  "Auxiliary Verb Phrases": {"batch":"Verbs", "batchOrder": "3",  "order": "18", "type": "intra-phrase", "pattern": "#Auxiliary+ #Verb+", "tag": {"all": ["VerbPhrase", "Verbial"], "last": {"type": "intra-phrase", "termTag": "HeadVerb"}}},

  "Modal Verb Phrases": {"batch":"Verbs", "batchOrder": "3",  "order": "19", "type": "intra-phrase", "pattern": "#Modal+ #Verb+", "tag": {"all": ["VerbPhrase", "Verbial"], "last": {"type": "intra-phrase", "termTag": "HeadVerb"}}},

  "Linking Verbs": {"batch":"Verbs", "batchOrder": "3",  "order": "20", "type": "intra-phrase", "pattern": "(#Model+|#Auxiliary+)? #Copula", "tag": {"all": "Linking"}},

  "Being Verbs": {"batch":"Verbs", "batchOrder": "3",  "order": "21", "type": "intra-phrase", "pattern": "#Modal?+ (be|am|is|are|was|were)", "tag": {"all": "Being"}},

  "Doing Verbs": {"batch":"Verbs", "batchOrder": "3",  "order": "23", "type": "intra-phrase", "pattern": "#Modal?+ not? (do|does|did) not? #Verb?+", "tag": {"all": "Doing"}},

  "Imperative": {"batch":"Verbs", "batchOrder": "3",  "order": "25", "type": "intra-phrase", "pattern": "do #BaseVerb", "tag": {"all": "Imperative"}},

  "Having Verbs": {"batch":"Verbs", "batchOrder": "3",  "order": "26", "type": "intra-phrase", "pattern": "#Modal?+ (have|has|had) #Verb?+", "tag": {"all": "Having"}},

  "Single Verbs": {"batch":"Verbs", "batchOrder": "3",  "order": "30", "type": "inter-phrase", "pattern": "(#Verb && !#Phrase)", "tag": {"on": {"term": "#Verb", "termTag": "Verbial"}}}
}
