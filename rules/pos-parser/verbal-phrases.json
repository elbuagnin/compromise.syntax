{
     "Gerund Infinitive Phrase": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "1", "type": "inter-phrase", "pattern": "#gd+ #iv+", "tag": {"on": {"term": "#iv+", "termTag": ["GerundPhrase", "nn"]}}, "untag": {"all": ["#vb", "#pp", "#dt", "#aj", "#av"]}, "modifier": {"term": "#iv+", "modifies": "#gd+", "termTag": "ObjectOfGerund"}},

     "Gerund List": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "2", "type": "inter-phrase", "pattern": "#gd+ #CoordinatingConjunction #gd", "tag": {"all": ["GerundPhrase", "List", "nn"]}},

     "Gerund Compound Phrase": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "3", "type": "inter-phrase", "pattern": "(#gd && !#Comma && !#List) #CoordinatingConjunction #gd", "tag": {"all": ["GerundPhrase", "CompoundPhrase", "nn"]}},

     "Participle Phrase 1": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "4", "type": "intra-phrase", "pattern": "^(#Participle && #Comma)", "tag": {"all": "pt"}},

     "Participle Phrase 2": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "5", "type": "intra-phrase", "pattern": "^#Participle (* && !#vb) #Comma", "tag": {"all": "pt"}},

     "Participle Phrase 3": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "6", "type": "intra-phrase", "pattern": "^(#Participle && #Comma)", "tag": {"all": "pt"}},

     "Participle Phrase 4": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "7", "type": "intra-phrase", "pattern": "#Participle #nn+", "tag": {"all": "pt"}},

     "Participle Phrase 4": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "8", "type": "intra-phrase", "pattern": "#Participle #av+", "tag": {"all": "pt"}},

     "Participle Phrase 4": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "9", "type": "intra-phrase", "pattern": "#av+ #Participle", "tag": {"all": "pt"}},

     "Participle Phrase 4": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "10", "type": "intra-phrase", "pattern": "#Participle #nn+", "tag": {"all": "pt"}},

     "Perfect Participle Phrase": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "11", "type": "intra-phrase", "pattern": "#PerfectParticiple+ #pt+", "tag": {"all": ["PerfectParticiplePhrase", "pt", "aj"]}, "untag": {"all": ["#nn", "#pp", "#dt", "#aj", "#av", "#vl"]}, "modifier": {"term": "#nn+", "modifies": "#PerfectParticiple+", "termTag": "ObjectOfPerfectParticiple"}},

     "Past Participle Phrase": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "12", "type": "intra-phrase", "pattern": "#PastParticiple+ #pt+", "tag": {"all": ["PastParticiplePhrase", "pt", "aj"]}, "untag": {"all": ["#nn", "#pp", "#dt", "#aj", "#av", "#vl"]}, "modifier": {"term": "#nn+", "modifies": "#PerfectParticiple+", "termTag": "ObjectOfPerfectParticiple"}},

     "Present Participle Phrase": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "13", "type": "intra-phrase", "pattern": "#PresentParticiple+ #pt+", "tag": {"all": ["PerfectParticiplePhrase", "pt", "aj"]}, "untag": {"all": ["#nn", "#pp", "#dt", "#aj", "#av", "#vl"]}, "modifier": {"term": "#nn+", "modifies": "#PerfectParticiple+", "termTag": "ObjectOfPerfectParticiple"}},

     "#Participle Phrase #Adverb": {"batch": "Verbal Phrases", "batchOrder": "8", "order": "14", "type": "intra-phrase", "pattern": "#pt+ #av+", "modifier": {"term": "#av+", "modifies": "#pt+"}},

     "#Participle Phrase #Proposition": {"batch": "Verbal Phrases", "batchOrder": "8", "order": "15", "type": "intra-phrase", "pattern": "#pt+ #pp+", "modifier": {"term": "#pp+", "modifies": "#pt+"}},

     "Perfect Participle Phrase": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "16", "type": "inter-phrase", "pattern": "(#PerfectParticiplePhrase+ && #Comma) #nn+", "modifier": {"term": "#PerfectParticiplePhrase+", "modifies": "#nn+", "termTag": "Adjectival"}},

     "Perfect Participle Phrase": {"batch":"Verbal Phrases", "batchOrder": "8",  "order": "17", "type": "intra-phrase", "pattern": "(#PerfectParticiplePhrase+ && #Comma) #nn+", "modifier": {"term": "#PerfectParticiplePhrase+", "modifies": "#nn+", "termTag": "Adjectival"}}
}
